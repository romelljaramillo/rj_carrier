{
  "name": "roanja/rj_carrier",
  "description": "Roanja - Rj Carrier",
  "homepage": "https://github.com/romelljaramillo/rj_carrier",
  "license": "AFL-3.0",
  "authors": [
    {
      "name": "Roanja",
      "email": "info@roanja.com"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "ramsey/uuid": "3.9.3",
    "iio/libmergepdf": "^4.0",
    "tecnickcom/tc-lib-pdf": "dev-main"
  },
  "require-dev": {
    "prestashop/php-dev-tools": "^3.4"
  },
  "autoload": {
    "psr-4": {
      "Roanja\\Module\\RjCarrier\\": "src/"
    },
    "classmap": ["rj_carrier.php"],
    "exclude-from-classmap": []
  },
  "config": {
    "preferred-install": "dist",
    "classmap-authoritative": true,
    "optimize-autoloader": true,
    "prepend-autoloader": false,
    "platform": {
      "php": "5.6"
    }
  },
  "repositories": [
    {
        "type": "vcs",
        "url": "git@github.com:tecnickcom/tc-lib-pdf.git"
    }
  ],
  "type": "prestashop-module"
}
